{
    "compilerOptions": {
        "outDir": "./dist/",
        "allowSyntheticDefaultImports": false,
        "target": "es6",
        "downlevelIteration": true,
        "importHelpers": true,
        "noImplicitAny": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "sourceMap": true,
        "module": "es6",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node"
    }
}
